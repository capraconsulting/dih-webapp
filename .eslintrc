{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "browser": true
    },
    "rules": {
      "indent": [2, 4],
      "react/jsx-indent": [2, 4],
      "react/jsx-indent-props": [2, 4],
      "comma-dangle": [2, "never"],
      "global-require": 0
    }
}
