{
    "containerDefinitions": [
        {
            "cpu": 1,
            "essential": true,
            "image": "036160847874.dkr.ecr.eu-west-1.amazonaws.com/dih-webapp:latest",
            "logConfiguration": {
                "logDriver": "json-file"
            },
            "memory": 300,
            "name": "dih-webapp",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "dockerLabels": {
                "environment": "prod",
                "name": "dih-webapp"
            }
        }
    ],
    "family": "dih-webapp-task"
}
